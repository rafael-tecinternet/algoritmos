algoritmo "Loop PARA"
// Função : Ler e exibir o nome e idade de 3 pessoas
var
banda: caracter
contador: inteiro

inicio

   para contador de 1 ate 5 faca
    escreva("Informe a ",contador,"ª ","banda: ")
    leia(banda)


    escreval("Banda informada: ",banda)
    escreval()

    
   fimpara

fimalgoritmo
